{
  "name": "repairgest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:electron\" \"npm run dev:next\"",
    "dev:electron": "electron ./electron/electron.js",
    "dev:next": "next dev",
    "build": "next build",
    "start": "electron ./electron/electron.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4",
    "electron-builder-squirrel-windows": "^24.9.1",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^6.1.4",
    "is-electron": "^2.2.2",
    "next": "14.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^18",
    "react-dom": "^18",
    "react-modal": "^3.16.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.3",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "description": "Aplicação de gestão de reparações - Electrex / João R. Matos",
  "main": "electron/electron.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AndreFCTeles/repairgest.git"
  },
